{
  "addr": ":8080",
  "models_dir": "~/models/llm",
  "vram_budget_mb": 8192,
  "vram_margin_mb": 512,
  "default_model": "llama-2-7b-q4",
  "log_level": "info",
  "max_body_bytes": 1048576,
  "infer_timeout": "30s",
  "cors_enabled": true,
  "cors_allowed_origins": [
    "http://localhost:3000",
    "http://127.0.0.1:3000"
  ],
  "cors_allowed_methods": ["GET", "POST", "OPTIONS"],
  "cors_allowed_headers": [
    "Accept",
    "Authorization",
    "Content-Type",
    "X-Requested-With",
    "X-Log-Level"
  ],
  "max_queue_depth": 16,
  "max_wait": "15s"
}
